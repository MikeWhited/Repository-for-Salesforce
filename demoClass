public class demoClass {
	public string searchboxstring{get; set;}
     public list<contact> result {get; set;}
     public list<opportunity> result1 {get; set;}
     public list<account> result2 {get; set;}
     public list<contract> result3 {get; set;}
     public list<matter__c> result4 {get; set;}
     public democlass()
     {
      result = new list<Contact>();
     }
     public void searchbox(){
      result=new List<Contact>();
      result=[select ID,Name,phone, account.name from Contact where account.Name like:'%'+searchboxstring+'%'];
      result1=new List<Opportunity>();
      result1=[select ID,Name, account.name from Opportunity where account.Name like:'%'+searchboxstring+'%'];
      result2=new List<Account>();
      result2=[select ID,Name, Owner.name,Closed_Won_Opptys__c, Site from Account where Name like:'%'+searchboxstring+'%'];
      result3=new List<Contract>();
      result3=[select ID,Name, account.name,Price_Per_GB__c,SpecialTerms,SubscriptionPricePerMonth__c, EndDate, StartDate, Owner.name from Contract where account.Name like:'%'+searchboxstring+'%'];
      result4 = new list<Matter__c>();
      result4=[select ID,Name,Active__c,Salesperson__c,Total_Active_GB__c,Per_GB_Price_per_Month__c  from Matter__c where Contract_Billing_Account__c like:'%'+searchboxstring+'%'];   
     }
}
